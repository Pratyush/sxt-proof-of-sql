{
    "extends": "solhint:all",
    "rules": {
        "comprehensive-interface": "off",
        "foundry-test-functions": "off",
        "function-max-lines": "off",
        "gas-calldata-parameters": "off",
        "import-path-check": "off",
        "max-line-length": "off",
        "no-global-import": "off",
        "no-inline-assembly": "off",
        "one-contract-per-file": "off",
        "ordering": "off",
        "use-natspec": "off"
    }
}
